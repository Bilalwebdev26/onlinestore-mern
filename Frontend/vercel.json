{
  "version": 2,
  "name": "Frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
